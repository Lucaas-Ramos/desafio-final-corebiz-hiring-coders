{
  "store.custom#about-us": {
    "blocks": [
      "flex-layout.row#about-us",
      "flex-layout.row#vision-values"
    ]
  },

  "flex-layout.row#about-us": {
    "children": [
      "flex-layout.col#text-about-us",
      "flex-layout.col#image-about-us"
    ],
    "props": {
      "blockClass": "aboutRow"
    }
  },

  "flex-layout.col#text-about-us": {
    "children": [
      "rich-text#title-about-us",
      "rich-text#about-us"
    ],
    "props": {
      "blockClass": "textAbout",
      "preventVerticalStretch": true,
      "width": "45%"
    }
  },

  "flex-layout.col#image-about-us": {
    "children": [
      "image#about-us"
    ],
    "props": {
      "blockClass": "aboutImage",
      "width": "55%"
    }
  },



  "flex-layout.row#vision-values": {
    "children": [
      "flex-layout.col#text-vision",
      "flex-layout.col#text-values"
    ],
    "props": {
      "blockClass": "visionValuesRow",
      "preventVerticalStretch": true,
      "fullWidth": true
    }
  },

  "flex-layout.col#text-vision": {
    "children": [
      "rich-text#title-vision",
      "rich-text#vision"
    ],
    "props": {
      "blockClass": "textVision",
      "preventVerticalStretch": true,
      "width": "85%"
    }
  },



  "flex-layout.col#text-values": {
    "children": [
      "rich-text#title-values",
      "rich-text#values"
    ],
    "props": {
      "blockClass": "textValues",
      "preventVerticalStretch": true,
      "width": "70%"
    }
  },




  "rich-text#title-vision": {
    "props": {
      "text":
      "# vision",
      "blockClass": "visionTitle"
    }
  },

  "rich-text#vision": {
    "props": {
      "text":
      "To be the largest company of \n experience, technology and \n marketing for commerce in \n Latin America.",
      "blockClass": "visionText"
    }
  },



  "rich-text#title-values": {
    "props": {
      "text":
      "# values",
      "blockClass": "valuesTitle"
    }
  },

  "rich-text#values": {
    "props": {
      "text":
      "### .determination \n ### .innovation \n ### .courage \n ### .agility \n ### .collaboration",
      "blockClass": "valuesText"
    }
  },



  "rich-text#title-about-us": {
    "props": {
      "text":
      "# a corebiz",
      "blockClass": "aboutTitle"

    }
  },

  "image#about-us": {
    "props": {
      "src": "https://www.corebiz.ag/wp-content/uploads/2020/06/about-intro-image.jpg",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "aboutImage",
      "horizontalAlign": "left"
    }
  },
  
  "rich-text#about-us": {
    "props": {
      "text": "**conheça um pouco de nossa** \n **história, nosso crescimento e** \n **nossa cultura.**",
      "blockClass": "aboutText"
    }
  }
}
